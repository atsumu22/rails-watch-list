<div class="container">
  <div class="form-card col-6">
    <%= simple_form_for(@list) do |f| %>

      <div class="form-inputs">
        <%= f.input :name %>
      </div>

      <div class="form-actions">
        <%= f.input :photo, as: :file %>
      </div>

      <div class="form-actions">
        <%= f.button :submit, class: "btn btn-success" %>
      </div>
    <% end %>
  </div>
</div>
